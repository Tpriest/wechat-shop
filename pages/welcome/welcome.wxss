page {
  width:100%;
  height:100%;
  background: #118307
}
.welcome {
    width: 100%;
    height:100%;
    display:flex;
    justify-content: space-around;
    flex-direction: column;
    align-items: center;
}
.title{
  color:#fff;
  margin-top:80rpx;
  font-weight:900
}
.welcome .bottom{
  justify-content: flex-end
}
.welcome image {
    width: 120px;
    height: 120px;
}

.welcome button {
    background-color: #118307;
    color: #fff;
    width: 260rpx;
    height:80rpx;
    opacity: 0;
    font-size:32rpx;
    animation: op 1s infinite;
    line-height: 80rpx;
    border:1px solid #fff;
    margin:40rpx 0
    
}

@keyframes op {
    0% {
        opacity: 0;
    }

    30% {
        opacity: 0.3;
    }

    60% {
        opacity: 0.6;
    }

    90% {
        opacity: 0.9;
        color: #fff;
    }

    100% {
        opacity: 1;
    }
}
.time {
    height: 40rpx;
    color: #fff;
    font-size: 24rpx;
    position: absolute;
    bottom: 5px;
    border-radius: 5px;
    line-height: 40rpx;
    right: 60rpx;
    padding:0 20rpx;
    border:1rpx solid #fff
}
